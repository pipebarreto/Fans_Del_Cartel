{
  "expo": {
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./local/assets/notification-sound.wav",
            "./local/assets/notification-sound-other.wav"
          ]
        }
      ]
    ],
    "name": "Fans_Del_Cartel",
    "slug": "Fans_Del_Cartel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/2025.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/2025.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/2025.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./images/2025.png"
    }
  }
}
